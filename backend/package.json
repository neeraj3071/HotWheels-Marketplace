{
  "name": "hotwheels-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --watch src --exec ts-node --files src/server.ts",
    "build": "tsc --project tsconfig.json",
    "start": "node dist/server.js",
    "lint": "cross-env ESLINT_USE_FLAT_CONFIG=false eslint \"src/**/*.ts\"",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "test": "cross-env NODE_ENV=test jest --runInBand",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --runInBand --coverage",
    "test:load": "artillery run artillery.yml",
    "test:stress": "artillery run artillery-stress.yml",
    "test:load:report": "artillery run artillery.yml --output report.json && artillery report report.json"
  },
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "artillery": "^2.0.27",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "ms": "^2.1.3",
    "winston": "^3.18.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.10",
    "@types/node": "^22.8.1",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prisma": "^5.17.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
